{
  "name": "valorant",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . -f mo",
    "fix": "eslint . --fix -f mo",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@unocss/reset": "^66.5.9",
    "clsx": "^2.1.1",
    "cva": "npm:class-variance-authority",
    "lucide-vue-next": "^0.555.0",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^4.0.1",
    "@iconify-json/ri": "^1.2.6",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-formatter-mo": "^2.4.1",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "~5.9.3",
    "unocss": "^66.5.9",
    "unocss-preset-animations": "^1.3.0",
    "unocss-preset-shadcn": "^1.0.1",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
